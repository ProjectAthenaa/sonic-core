type Module{
    Name: String!
    Status: Status!
    Fields: [ModuleField!]
}

enum Status{
    FUNCTIONAL
    DEGRADED
    DOWN
}

enum FieldType{
    KEYWORDS
    TEXT
    NUMBER
    GENDER
    WIDTH
    SHOE_SIZE
    DROPDOWN
}

type ModuleField{
    Type: FieldType!
    Label: String!
    Validation: String
    FieldKey: String
}

extend type Query{
    moduleInformation: [Module!]
}