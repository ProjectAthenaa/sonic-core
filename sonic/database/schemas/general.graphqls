type Module{
    Name: String!
    Status: Status!
    Accounts: Boolean!
    Fields: [ModuleField!]
}

enum Status{
    FUNCTIONAL
    DEGRADED
    DOWN
}

enum FieldType{
    KEYWORDS
    TEXT
    NUMBER
    DROPDOWN
}

type ModuleField{
    Type: FieldType!
    Label: String!
    Validation: String
    FieldKey: String
    DropdownValues: [String!]
}

extend type Query{
    moduleInformation: [Module!]
}